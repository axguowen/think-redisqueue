{
    "name": "axguowen/think-redisqueue",
    "description": "Simple Redis Queue For ThinkPHP",
    "keywords": [
        "queue",
        "extend",
        "redis",
        "thinkphp"
    ],      
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "axguowen",
            "email": "axguowen@qq.com"
        }
    ],
    "require": {
        "workerman/workerman": "^4.1",
        "topthink/framework": "^6.1 || ^8.0",
        "axguowen/think-redisclient": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src"
        }
    },
    "extra": {
        "think":{
            "services":[
                "think\\redisqueue\\Service"
            ],
            "config": {
                "redisqueue": "src/redisqueue/config.php"
            }
        }
    }
}
